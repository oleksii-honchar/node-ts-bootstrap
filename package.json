{
  "name": "svc-bootstrap",
  "version": "2.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm run docker:up:loc",
    "prebuild": "npm run clean:dist",
    "build": "bash scripts/build.sh",
    "build:analyze": "cross-env-shell BUILD_ANALYZE=true bash ./scripts/build.sh",
    "build:loc": "bash scripts/build.loc.sh",
    "build:loc:analyze": "cross-env-shell BUILD_ANALYZE=true bash ./scripts/build.loc.sh",
    "check:all": "run-p lint types:check",
    "clean:dist": "rm -rf dist",
    "seed:loc": "cross-env SEED_MODE=true npm run docker:up:loc",
    "docker:build": "bash scripts/build.docker.sh",
    "docker:up:loc": "bash devops/docker/up.loc.sh",
    "docker:up:dev": "bash devops/docker/up.dev.sh",
    "docker:up:test": "bash devops/docker/up.test.sh",
    "launch": "npm run launch:prod",
    "launch:loc": "bash scripts/launch.loc.sh",
    "launch:prod": "bash scripts/launch.sh",
    "launch:dev": "bash scripts/launch.dev.sh",
    "launch:qa": "bash scripts/launch.qa.sh",
    "lint": "npx eslint --fix src --config configs/eslint.config.js --ext .ts,.tsx,.json --ignore-pattern '/node_modules/*' --ignore-pattern '**/*.d.ts'",
    "test": "bash scripts/test.sh",
    "test:coverage": "cross-env NODE_ENV=test jest  --runInBand -c ./configs/jest.config.js --coverage",
    "test:watch": "bash scripts/test-watch.sh",
    "types:check": "tsc -p configs/tsconfig.es20.json",
    "types:watch": "npm run types:check -- --watch",
    "watch": "run-p types:watch watch:loc",
    "watch:loc": "bash scripts/watch.loc.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check:all"
    }
  },
  "author": "Oleksii Honchar",
  "license": "MIT",
